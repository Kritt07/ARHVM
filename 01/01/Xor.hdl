/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=ANandB);
    Nand(a=ANandB, b=a, out=nandA);
    Nand(a=ANandB, b=b, out=nandB);
    Nand(a=nandA, b=nandB, out=out);
}